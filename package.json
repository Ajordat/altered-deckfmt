{
  "name": "altered-deckfmt",
  "version": "0.1.0",
  "description": "A compact format for storing Altered TCG decklists",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/altered-deckfmt.umd.cjs",
  "module": "./dist/altered-deckfmt.js",
  "exports": {
    ".": {
      "import": "./dist/my-lib.js",
      "require": "./dist/my-lib.umd.cjs"
    }
  },
  "scripts": {
    "clean": "rimraf dist dist.esm",
    "dev": "vite",
    "build": "vite build",
    "build-tsc": "npm run clean && tsc -b tsconfig.build.json",
    "test": "npm run build-tsc && node dist/test/encoding.test.js",
    "test-dec": "npm run build-tsc && node dist/test/new-decoder.test.js"
  },
  "dependencies": {
    "@astronautlabs/bitstream": "^4.2.2",
    "buffer": "^6.0.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "typescript": "^5.3.3",
    "vite": "^5.4.2"
  },
  "author": "",
  "license": "MIT"
}
